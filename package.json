{
  "name": "@codestra/next-mongooseless",
  "version": "1.0.0",
  "private": true,
  "description": "Mongoose helper functions to connect with mongodb in nextjs api-routes",
  "main": "src/index.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "dev": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codestra/next-mongooseless.git"
  },
  "keywords": [
    "mongoose",
    "next"
  ],
  "author": "codestra",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/next-mongooseless/issues"
  },
  "homepage": "https://github.com/codestra/next-mongooseless#readme",
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "codecov": "^3.1.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^4.3.5"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "mongodb-client-encryption": "^1.2.6",
    "mongoose": "^5.13.3",
    "tslint": "^6.1.3"
  }
}
